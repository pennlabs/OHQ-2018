{
  "name": "OHQ",
  "version": "1.0.0",
  "description": "Office Hours Queue for Penn",
  "main": "frontend/index.js",
  "repository": "https://github.com/pennlabs/OHQ.git",
  "homepage": "https://github.com/pennlabs/OHQ",
  "engines": {
    "node": "8"
  },
  "bugs": {
    "url": "https://github.com/pennlabs/OHQ/issues"
  },
  "scripts": {
    "clean": "rimraf ApiServer/public/js && rimraf ApiServer/public/css && rimraf ApiServer/public/images",
    "start": "NODE_ENV=development node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "build": "npm run clean && NODE_ENV=production webpack --progress --display-reasons --display-modules",
    "postinstall": "npm run clean && NODE_ENV=production webpack --progress",
    "prod": "node ./ApiServer/index.js",
    "server": "nodemon ./ApiServer/index.js",
    "lint": "eslint **/",
    "test": "npm run lint -- --quiet && mocha --compilers js:babel-core/register --require ignore-styles --require babel-polyfill --require ./frontend/test/test_helper.js --recursive ./frontend/test",
    "test:watch": "npm run test -- --watch"
  },
  "author": "Penn Labs",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^6.1.2",
    "chai": "^3.5.0",
    "chai-jquery": "^2.0.0",
    "eslint": "^3.16.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.2",
    "eslint-plugin-react": "^5.2.2",
    "eslint-plugin-standard": "^2.0.1",
    "ignore-styles": "^5.0.1",
    "jsdom": "^9.11.0",
    "mocha": "^2.5.3",
    "nodemon": "^1.11.0",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "^1.3.1",
    "webpack-dev-server": "^1.16.3"
  },
  "dependencies": {
    "axios": "^0.13.1",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-polyfill": "^6.23.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-1": "^6.22.0",
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "^3.4.7",
    "body-parser": "^1.16.1",
    "cors": "^2.8.1",
    "css-loader": "^0.23.1",
    "dotenv": "^2.0.0",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "ignore-styles": "^5.0.1",
    "jquery": "^3.1.1",
    "jwt-simple": "^0.5.1",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "lodash": "^4.17.4",
    "mongoose": "^4.8.4",
    "morgan": "^1.8.1",
    "passport": "^0.3.2",
    "passport-jwt": "^2.2.1",
    "passport-local": "^1.0.0",
    "postcss-loader": "^1.3.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^4.4.6",
    "react-router": "^2.8.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^2.5.4",
    "rimraf": "^2.5.4",
    "socket.io": "^1.7.3",
    "style-loader": "^0.13.1",
    "uuid": "^3.2.1",
    "webpack": "^1.14.0"
  }
}
